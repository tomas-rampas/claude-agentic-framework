{
  "mcpServers": {
    "sequential-thinking": {
      "command": "bash",
      "args": [
        "-lc",
        "source ~/.nvm/nvm.sh && /home/mcquak/.nvm/versions/node/v24.1.0/bin/npx mcp-sequentialthinking-tools"
      ],
      "env": {
        "SEQUENTIAL_THINKING_MODE": "comprehensive",
        "MAX_DEPTH": "10",
        "ENABLE_VALIDATION": "true",
        "ANALYSIS_DETAIL": "high",
        "INCLUDE_ALTERNATIVES": "true"
      }
    },
    "context7": {
      "type": "http",
      "url": "https://mcp.context7.com/mcp",
      "headers": {
        "CONTEXT7_API_KEY": "${CONTEXT7_API_KEY}"
      },
      "env": {
        "CACHE_ENABLED": "true",
        "UPDATE_FREQUENCY": "daily",
        "INCLUDE_EXAMPLES": "true",
        "VERSION_TRACKING": "true"
      }
    }
  }
}